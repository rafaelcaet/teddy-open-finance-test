generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id       Int      @id @default(autoincrement())
  email    String   @db.VarChar(255)
  password String   @db.VarChar(255)
  links    String[] @default([])
}
